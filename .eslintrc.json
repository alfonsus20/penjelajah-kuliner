{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "object-curly-newline": "off",
        "linebreak-style": 0,
        "no-underscore-dangle" : 0,
        "operator-linebreak" : "off",
        "comma-dangle": "off"
    }
}
